    <string name="food_name_1">Regal Palace - Restaurant</string>
    <string name="food_descr_1">Indian food prepared by Indian chef, including vegetarian opt.</string>
    <string name="food_price_1">Lunch US$7, Dinner US$9.</string>
    <string name="food_name_2">Sikhookyung - Restaurant</string>
    <string name="food_descr_2">A good Korean restaurant with friendly service.</string>
    <string name="food_price_2">From 8,000 som.</string>
    <string name="food_name_3">Nur - Cafe</string>
    <string name="food_descr_3">Clean local cafe selling pizza, including vegetarian options.</string>
    <string name="food_price_3">From 3,500 som.</string>
    <string name="food_name_4">Istiqlol - Cafe</string>
    <string name="food_descr_4">Serving shurpa, lagman, mastava, plov, manti, shashlik.</string>
    <string name="food_price_4">Breakfast US$5, Lunch US$6, Dinner US$7.</string>
    <string name="food_name_5">Karim Bek - Restaurant</string>
    <string name="food_descr_5">The restaurant hall turns into a disco at 20:00.</string>
    <string name="food_price_5">Breakfast US$3, Lunch US$6, Dinner US$8.</string>
    <string name="food_name_6">Magistr - Cafe</string>
    <string name="food_descr_6">The manager speaks English. Free WiFi.</string>
    <string name="food_price_6">Pizzas from 12,000 som, Mains from 4,000 som.</string>
    <string name="food_name_7">Anjir - Restaurant</string>
    <string name="food_descr_7">Good quality food, popular with locals.</string>
    <string name="food_price_7">7,500 som for two kebabs, twice that for a dish.</string>
    <string name="hotel_name_1">Bahodir - BnB</string>
    <string name="hotel_descr_1">The staff is friendly, honest and willing to sell beer from fridge.</string>
    <string name="hotel_price_1">Single US$8, Double US$20.</string>
    <string name="hotel_name_2">Jahongir - BnB</string>
    <string name="hotel_descr_2">Comfortable rooms with modern amenities.</string>
    <string name="hotel_price_2">Single US$25, Double US$40.</string>
    <string name="hotel_name_3">Davr - BnB</string>
    <string name="hotel_descr_3">BnB with rooms set around courtyard.</string>
    <string name="hotel_price_3">US$15 per person per night, incl. breakfast.</string>
    <string name="hotel_name_4">Zarafshan - Hotel</string>
    <string name="hotel_descr_4">A renovated old Soviet hotel with loads of moody charm.</string>
    <string name="hotel_price_4">US$15-30.</string>
    <string name="hotel_name_5">Regal Palace - Hotel</string>
    <string name="hotel_descr_5">Close to the Samarkand airport.</string>
    <string name="hotel_price_5">US$65-85, including breakfast.</string>
    <string name="hotel_name_6">Malika - Hotel</string>
    <string name="hotel_descr_6">Located in the city centre.</string>
    <string name="hotel_price_6">US$40-65, including breakfast.</string>
    <string name="hotel_name_7">Registan Plaza - Hotel</string>
    <string name="hotel_descr_7">A four-star hotel, opened in 2004.</string>
    <string name="hotel_price_7">US$165, incl. buffet breakfast.</string>
    <string name="transport_name_1">By plane</string>
    <string name="transport_descr_1">Daily flights from Tashkent except Mon and Fri.</string>
    <string name="transport_price_1">US$21 per person.</string>
    <string name="transport_name_2">By train</string>
    <string name="transport_descr_2">Daily trains from Tashkent: super-fast Afrosiob and Sharq.</string>
    <string name="transport_price_2">3-4 hours.</string>
    <string name="transport_name_3">By car</string>
    <string name="transport_descr_3">Shared taxis leave from Tashkent (S. Rahimov).</string>
    <string name="transport_price_3">About 4 hours by road.</string>
    <string name="monument_name_1">Registan Ensemble</string>
    <string name="monument_descr_1">Ancient city square, surrounded by 3 medrese.</string>
    <string name="monument_price_1">Entrance fee: 16,400 som.</string>
    <string name="monument_name_2">Bibi-Khanym Mosque</string>
    <string name="monument_descr_2">Named after the wife of Temur, local ruler.</string>
    <string name="monument_price_2">Entrance fee: 10,000 som.</string>
    <string name="monument_name_3">Khazrat-Khizr</string>
    <string name="monument_descr_3">A beautiful mosque, built at the city entrance.</string>
    <string name="monument_price_3">Open: 08:00-18:00.</string>
    <string name="monument_name_4">Ulugbek\'s Observatory</string>
    <string name="monument_descr_4">Only part of the huge sextant is preserved.</string>
    <string name="monument_price_4">Open: 09:00-19:00.</string>
    <string name="monument_name_5">Gur Emir Mausoleum</string>
    <string name="monument_descr_5">Tomb of the conqueror Tamerlane.</string>
    <string name="monument_price_5">Entrance fee: 25,000 som for foreigners.</string>
    <string name="monument_name_6">Shakhi-Zinda Ensemble</string>
    <string name="monument_descr_6">An ancient necropolis (9-14th centuries).</string>
    <string name="monument_price_6">Entrance fee: 7,000 som.</string>
    <string name="monument_name_7">Al-Bukhari Mausoleum</string>
    <string name="monument_descr_7">Al Buxori was a collector of the sayings of prophet Muhamed.</string>
    <string name="monument_price_7">Visited daily by about 1,000 visitors.</string>
</resources>
